@model Acedemy.Mvc.UI.Models.ChartModel

@using System.Web.Helpers

@{
    Chart chart = new Chart(500, 500);
    chart.AddTitle("Yoklama Grafik");
    chart.AddLegend("Durum");

    chart.AddSeries(name: "Katılımcılar", chartType: "Column",
        xValue: new[] { 20 },
yValues: new[] { Model.totalParticipant }
        );
    chart.AddSeries(name: "Toplam Öğrenci", chartType: "Column",
       xValue: new[] { 20 },
       yValues: new[] { Model.totalSudent }
       );
    chart.SaveToCache("chart");
}